<mvc:View controllerName="recn.RECN.controller.ContractView" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<App id="idAppControl">
		<pages>
			<Page showHeader="false">
				<Input value="{contractModel>REContractName}" valueLiveUpdate="true" width="50%"/>
				<DatePicker value="{contractModel>ContractStartDate}" editable="true" width="50%"/>
				<DatePicker value="{contractModel>ContractEndDate}" editable="true" width="50%"/>
			</Page>
		</pages>
	</App>
</mvc:View>